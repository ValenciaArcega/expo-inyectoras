{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@views/*": [
        "src/views/*"
      ],
      "@core/*": [
        "src/views/core/*"
      ],
      "@public/*": [
        "src/views/public/*"
      ]
    },
    "types": [
      "./env.d.ts"
    ],
  }
}